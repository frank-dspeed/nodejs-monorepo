{
  "name": "@straw-hat/types",
  "version": "0.4.1",
  "description": "A bag of TypeScript types",
  "author": "Straw Hat Team <opensource@straw-hat-llc.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript-monorepo.git",
    "directory": "packages/types"
  },
  "license": "MIT",
  "keywords": [
    "types",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prebuild": "yarn run -T rimraf dist",
    "build": "yarn prebuild && yarn run -T tsc -p tsconfig.prod.json",
    "test:jest": "yarn run -T jest",
    "prepack": "yarn build",
    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./css-modules": {
      "types": "./dist/css-modules.d.ts",
      "import": "./dist/css-modules.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    },
    "./svgr-webpack": {
      "types": "./dist/svgr-webpack.d.ts",
      "import": "./dist/svgr-webpack.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@types/react": "^17.0.14"
  },
  "devDependencies": {
    "@straw-hat/tsconfig": "^3.0.1",
    "@types/react": "^17.0.14"
  }
}
