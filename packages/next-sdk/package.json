{
  "name": "@straw-hat/next-sdk",
  "version": "0.5.1",
  "description": "A Software Development Kit for NextJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript-monorepo.git",
    "directory": "packages/next-sdk"
  },
  "license": "MIT",
  "keywords": [
    "straw-hat-team",
    "nextjs",
    "next",
    "react",
    "sdk"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
