    "docs:typedoc": "yarn run -T typedoc"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "next": "^12.0.4",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@straw-hat/types": "^0.2.0"
  },
  "devDependencies": {
    "@straw-hat/tsconfig": "^3.0.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.36",
    "jest": "^27.3.1",
    "next": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
